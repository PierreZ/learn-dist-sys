{
    "name": "Java + JBang Development",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "vscjava.vscode-java-pack",
                "jbangdev.jbang-vscode"
            ]
        }
    },
    "postCreateCommand": "./setup.sh",
    // Comment out the next line to run as root instead
    "remoteUser": "vscode"
}
